<basics wx:if="{{PageCur=='basics'}}"></basics>
<component wx:if="{{PageCur=='component'}}"></component>
<order wx:if="{{PageCur=='order'}}"></order>
<selfInformation wx:if="{{PageCur=='selfInformation'}}"></selfInformation>
<view class="cu-bar tabbar bg-white shadow foot">
  <view class="action" bindtap="NavChange" data-cur="basics">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/basics{{PageCur=='basics'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='basics'?'text-green':'text-gray'}}">订单中心</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="component">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/component{{PageCur=='component'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='component'?'text-green':'text-gray'}}">接单</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="order">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/plugin{{PageCur=='order'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='order'?'text-green':'text-gray'}}">下单</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="selfInformation">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/about{{PageCur=='selfInformation'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='selfInformation'?'text-green':'text-gray'}}">个人中心</view>
  </view>
</view>